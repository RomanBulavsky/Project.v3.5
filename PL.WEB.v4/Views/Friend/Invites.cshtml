@model List<RFoundation.BLL.Interfaces.Entities.BllUser>

@{
    ViewBag.Title = "Invites";
}
<div class="container jumbotron" style="background: white">
    @if (Model.Count < 1)
    {
        <div class="text-center">
            <h2>You do not have any Friend Invites</h2>
        </div>
    }
    else
    {
        <div class="list-group">
            <ul>
                @if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        <li class="list-group-item">
                            @item.Login
                           </li>
                    }
                }
            </ul>
        </div>
    }
</div>

