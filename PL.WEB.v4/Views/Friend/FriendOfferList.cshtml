@model List<RFoundation.BLL.Interfaces.Entities.BllUser>
<div class="container jumbotron" style="background: white">
    @if (Model.Count < 1)
    {
        <div class="text-center">
            <h2>You do not have any Friend Offers</h2>
        </div>
    }
    else
    {
        <div class="container jumbotron" style="background: white">

            <div class="list-group">
                @if (Model != null)
                {
                    foreach(var friend in Model)
                    {
                        @Html.ActionLink(@friend.Login, "ShareFile2", "File", new {friendId = friend.Id, fileid = ViewBag.fileId, smth="xxx"}, new {id=@friend.Id, @class = "list-group-item"})

                    }
                }



            </div>
        </div>
    }
</div>
